                        "$ref": "#/definitions/_zero_one"
                    },
                    "loc_precision":{
                        "type": "string",
                        "format": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "place_type": {
                        "type": "string",
                        "enum": [
                            "garden",
                            "municipality",
                            "place",
                            "polygone",
                            "subplace",
                            "square",
                            "transect"
