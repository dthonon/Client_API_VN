                    "coord_lon": {
                        "type": "string",
                        "format": "number"
                    }, 
                    "id":{
                        "type": "string",
                        "format": "integer"
                    },
                    "id_commune":{
                        "type": "string",
                        "format": "integer"
                    },
                    "id_region":{
                        "type": "string",
                        "format": "integer"
                    },
